{
	"manifest_version": 2,
	"name": "miichan",
	"version": "0.4",
	"author":"Larvata",

	"permissions":[
		"notifications",
		"http://*/*"
	],

	"icons":{
		"128":"images/icon128.png"
		},

	"browser_action":{
		"default_icon":{
			"64":"images/icon89.png"
		},
		"default_title":"Miichan v0.4",
		"default_popup":"popup.html"

	},

	"options_page":"options.html",
	"background":{
		"scripts":["background.js"]
	}


}
