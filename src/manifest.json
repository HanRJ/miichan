{
	"manifest_version": 2,
	"name": "miichan",
	"version": "0.4",

	// "permissions":["storage"],
	"permissions":[
		"notifications",
		"http://*/*"
	],

	// for notification
	// "web_accessible_resources":[
	// 	"images/icon64.png"
	// ],
	"icons":{
		"128":"images/icon89.png"
		},

	"browser_action":{
		"default_icon":{
			"64":"images/icon89.png"
		},
		"default_title":"Miichan v0.4",
		"default_popup":"popup.html"

	},

	"options_page":"options.html",
	"background":{
		"scripts":["background.js"]
	}


}
